{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Server=educationsystem.studentmanagement.db;Initial Catalog=StudentsDb;User=sa;Password=Your_password123;"
  },
  "RabbitMQ": {
    "HostUri": "rabbitmq://educationsystem.rabbitmq",
    "ConnectionString": "amqp://guest:guest@educationsystem.rabbitmq"
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "HealthCheck",
        "Uri": "http://educationsystem.studentmanagement.api/health"
      }
    ],
    "EvaluationTimeOnSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  }
}
